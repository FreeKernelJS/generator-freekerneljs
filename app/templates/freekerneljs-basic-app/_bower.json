{<% var ngVer = "~1.3.15" %>
  "name": "<%= slugname %>",
  "version": "<%= props.version %>",
  "dependencies": {
      
    <% if (angularModule) { %>
    "angular": "<%= ngVer %>"<% } %>

    <% if (cookiesModule) { %>,
    "angular-cookies": "<%= ngVer %>"<% } %>

    <% if (resourceModule) { %>,
    "angular-resource": "<%= ngVer %>"<% } %>

    <% if (messagesModule) { %>,
    "angular-messages": "<%= ngVer %>"<% } %>

    <% if (sanitizeModule) { %>,
    "angular-sanitize": "<%= ngVer %>"<% } %>

    <% if (touchModule) { %>,
    "angular-touch": "<%= ngVer %>"<% } %>

    <% if (mocksModule) { %>,
    "angular-mocks": "<%= ngVer %>"<% } %>

    <% if (routeModule) { %>,
    "angular-ui-router": "~0.2.15"<% } %>

    <% if (bootstrapModule) { %>,
    "bootstrap": "~3.3.4"<% } %>

    <% if (scriptjsModule) { %>, 
    "script.js": "^2.5.7"<% } %>

  },
  "overrides": {
    "script.js": {
      "main": "dist/script.js"
    }
  }
}
