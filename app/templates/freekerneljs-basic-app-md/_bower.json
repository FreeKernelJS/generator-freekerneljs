{<% var ngVer = "~1.4.0" %>
  "name": "<%= slugname %>",
  "version": "<%= props.version %>",
  "dependencies": {
      
    <% if (angularModule) { %>
    "angular": "^<%= ngVer %>"<% } %>

    <% if (cookiesModule) { %>,
    "angular-cookies": "^<%= ngVer %>"<% } %>

    <% if (resourceModule) { %>,
    "angular-resource": "^<%= ngVer %>"<% } %>

    <% if (messagesModule) { %>,
    "angular-messages": "^<%= ngVer %>"<% } %>

    <% if (sanitizeModule) { %>,
    "angular-sanitize": "^<%= ngVer %>"<% } %>

    <% if (touchModule) { %>,
    "angular-touch": "^<%= ngVer %>"<% } %>

    <% if (mocksModule) { %>,
    "angular-mocks": "^<%= ngVer %>"<% } %>

    <% if (routeModule) { %>,
    "angular-ui-router": "~0.2.15"<% } %>

    <% if (materialModule) { %>,
    "angular-material": "^0.9.6"<% } %>

    <% if (scriptjsModule) { %>, 
    "script.js": "^2.5.7"<% } %>

    <% if (iconicFont) { %>,
    "material-design-iconic-font": "^1.1.1"<% } %>

  },
  "overrides": {
    "script.js": {
      "main": "dist/script.js"
    }
  }
}
