{<% var ngVer = "1.3.13" %>
  "name": "<%= slugname %>",
  "version": "<%= props.version %>",
  "dependencies": {
    <% if (angularModule) { %>
    "angular": "^<%= ngVer %>"<% } %><% if (routeModule) { %>,
    "angular-route": "^<%= ngVer %>"<% } %><% if (ariaModule) { %>,
    "angular-aria": "^<%= ngVer %>"<% } %><% if (animateModule) { %>,
    "angular-animate": "^<%= ngVer %>"<% } %><% if (cookiesModule) { %>,
    "angular-cookies": "^<%= ngVer %>"<% } %><% if (resourceModule) { %>,
    "angular-resource": "^<%= ngVer %>"<% } %><% if (messagesModule) { %>,
    "angular-messages": "^<%= ngVer %>"<% } %><% if (sanitizeModule) { %>,
    "angular-sanitize": "^<%= ngVer %>"<% } %><% if (touchModule) { %>,
    "angular-touch": "^<%= ngVer %>"<% } %><% if (materialModule) { %>,
    "angular-material": "^0.8.2"<% } %><% if (scriptjsModule) { %>,
    "script.js": "^2.5.7"<% } %><% if (iconicFont) { %>,
    "material-design-iconic-font": "^1.1.1"<% } %>
  },
  "overrides": {
    "script.js": {
      "main": "dist/script.js"
    }
  }
