{<% var ngVer = "~1.4.0" %>
  "name": "<%= slugname %>",
  "version": "<%= props.version %>",
  "dependencies": {
      
    <% if (angularModule) { %>
    "angular": "<%= ngVer %>"<% } %>

    <% if (cookiesModule) { %>,
    "angular-cookies": "<%= ngVer %>"<% } %>

    <% if (resourceModule) { %>,
    "angular-resource": "<%= ngVer %>"<% } %>

    <% if (messagesModule) { %>,
    "angular-messages": "<%= ngVer %>"<% } %>

    <% if (sanitizeModule) { %>,
    "angular-sanitize": "<%= ngVer %>"<% } %>

    <% if (touchModule) { %>,
    "angular-touch": "<%= ngVer %>"<% } %>

    <% if (mocksModule) { %>,
    "angular-mocks": "<%= ngVer %>"<% } %>

    <% if (routeModule) { %>,
    "angular-ui-router": "~0.2.15"<% } %>

    <% if (bootstrapModule) { %>,
    "bootstrap": "~3.3.5"<% } %>

    <% if (scriptjsModule) { %>, 
    "script.js": "^2.5.7"<% } %>

    <% if (angularTranslateModule) { %>,
    "angular-translate": "^2.7.2"<% } %>

    <% if (angularTranslateLoaderStaticFilesModule) { %>,
    "angular-translate-loader-static-files": "^2.7.2"<% } %>
  
  },
  "overrides": {
    "script.js": {
      "main": "dist/script.js"
    },
    "bootstrap": {
      "main": "dist/css/bootstrap.css"
    }
  }
}
